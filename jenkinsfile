pipeline {
   agent any
   stages {
      stage('git checkout') {
         steps {
            git 'https://github.com/shaikshareefmastan/jenkinspro.git'
         }
       }
      stage('Build application') {
         steps {
           script{
                
               def cmd = readFile 'variable' 
                     for (i in [ '${cmd}' ]) {
                          "echo $i"
               }      
            }
            
         }
      }
   }
}
