pipeline {
   agent any
   stages {
      stage('git checkout') {
         steps {
            git 'https://github.com/shaikshareefmastan/jenkinspro.git'
         }
       }
      stage('Build application') {
         steps {
           script{
                docker.image('httpd').withRun('-p 8080:80')
                				
            }
            
         }
      }
   }
}
