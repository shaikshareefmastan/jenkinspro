pipeline {
   agent any
   stages {
      stage('git checkout') {
         steps {
            git 'https://github.com/shaikshareefmastan/jenkinspro.git'
         }
       }
      stage('Build application') {
         steps {
		 withDockerContainer(image: "httpd"){
           script{
				   sh "touch /var/www/html/index.html"
				   sh "echo hello > /var/www/html/index.html"
				   sh "systemctl restart httpd"
				}
                				
            }
            
         }
      }
   }
}
