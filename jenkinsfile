pipeline {
   agent any
   stages {
      stage('git checkout') {
         steps {
            git 'https://github.com/shaikshareefmastan/jenkinspro.git'
         }
       }
      stage('Build application') {
         steps {
           script{
                
               def cmd = readFile 'variable' 
                     for (i in [ "${cmd}" ]) {
                       test = "$i"
             }
                writeFile file: 'steps.groovy', text: test
      stage('printenv') {
         steps {
           script {  
                load "/var/lib/jenkins/workspace/pipeline/steps.groovy"
                echo "${env.v1}"
                echo "${env.v2}"
                }
               } 
             }
           } 
         }
      }
   }
}
